generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Candidate {
  id           Int      @id @default(autoincrement())  
  name         String
  surname      String
  seniority    String
  years        Int
  availability Boolean
  createdAt    DateTime @default(now())
}
